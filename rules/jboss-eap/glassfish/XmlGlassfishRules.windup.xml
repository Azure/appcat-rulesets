<?xml version="1.0"?>
<ruleset xmlns="http://windup.jboss.org/schema/jboss-ruleset" id="XmlGlassfishRules"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://windup.jboss.org/schema/jboss-ruleset http://windup.jboss.org/schema/jboss-ruleset/windup-jboss-ruleset.xsd">
    <rules>
        <rule id="XmlGlassfishRules_2fmb">
            <when>
                <xmlfile as="default" public-id=".+GlassFish Application Server .+Servlet.+">
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="Glassfish Web Descriptor" effort="3"/>
                </iteration>
            </perform>
        </rule>
    </rules>
</ruleset>
