<?xml version="1.0"?>
<ruleset xmlns="http://windup.jboss.org/schema/jboss-ruleset" id="XmlResinRules"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://windup.jboss.org/schema/jboss-ruleset http://windup.jboss.org/schema/jboss-ruleset/windup-jboss-ruleset.xsd">
    <rules>
        <rule id="XmlResinRules_2fmb">
            <when>
                <xmlfile as="default" matches="/resin:web-app">
                    <namespace prefix="resin" uri="http://caucho.com/ns/resin"/>
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="Resin Web Application Descriptor" effort="3"/>
                </iteration>
            </perform>
        </rule>
    </rules>
</ruleset>
